{
  "name": "tracvac-server",
  "version": "1.12.1",
  "description": "Server software for Tracvac",
  "main": "./src/index.ts",
  "bin": "dist/cluster.js",
  "author": "benedict.jude.rocat@gmail.com",
  "license": "MIT",
  "scripts": {
    "test": "mocha tests --timeout 10000",
    "distributable-build": "pkg package.json -t node14-win",
    "start": "node dist/cluster",
    "generate-models": "objection-generator generate ./objection-generator/schema.yaml ./src/database",
    "generate-migrations": "objection-generator knex ./objection-generator/schema.yaml ./tmp_migrations",
    "generate-all": "yarn generate-models && yarn generate-migrations"
  },
  "dependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/cors": "^2.8.10",
    "@types/express": "^4.17.11",
    "@types/express-fileupload": "^1.1.6",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/knex": "^0.16.1",
    "@types/mocha": "^8.2.2",
    "@types/morgan": "^1.9.2",
    "@types/node-persist": "^3.1.1",
    "@types/uuid": "^8.3.0",
    "@types/web-push": "^3.3.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "chai": "^4.3.4",
    "chai-http": "^4.3.0",
    "chalk": "^4.1.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "exceljs": "^4.2.1",
    "express": "^4.17.1",
    "express-basic-auth": "^1.2.0",
    "express-fileupload": "^1.2.1",
    "jsonwebtoken": "^8.5.1",
    "knex": "^0.95.1",
    "mocha": "^8.3.2",
    "morgan": "^1.10.0",
    "node-persist": "^3.1.0",
    "objection": "^2.2.14",
    "pg": "^8.5.1",
    "sqlite3": "^5.0.2",
    "uuid": "^8.3.2",
    "web-push": "^3.4.4"
  },
  "devDependencies": {
    "nodemon": "^2.0.7",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.3",
    "username-generator": "^1.1.0"
  },
  "pkg": {
    "scripts": [
      "dist/**/*.js"
    ],
    "assets": [
      "static/**/*",
      "public/**/*",
      "app/**/*"
    ]
  }
}